<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
		package="com.syh.deeepoch.deecommunity">

	<application
			android:name="com.syh.deeepoch.epochcore.EpochApplication"
			android:allowBackup="true"
			android:icon="@mipmap/ic_launcher"
			android:label="@string/app_name"
			android:roundIcon="@mipmap/ic_launcher_round"
			android:supportsRtl="true"
			android:theme="@style/AppTheme">
		<meta-data android:name="com.syh.deeepoch.deecommunity.CommunityApp"
				android:value="IComponentConfig"/>
		<activity
				android:name=".DeeCommunityActivity"
				android:label="@string/title_activity_dee_community"
				android:theme="@style/AppTheme.NoActionBar">
		</activity>
		<activity
				android:name=".CommunityGhostActivity"
				android:label="@string/title_activity_community_ghost"
				android:theme="@style/AppTheme.NoActionBar">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>

				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
	</application>

</manifest>