<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
		package="com.syh.deeepoch.deeec">

	<application
			android:name="com.syh.deeepoch.epochcore.EpochApplication"
			android:allowBackup="true"
			android:icon="@mipmap/ic_launcher"
			android:label="@string/app_name"
			android:roundIcon="@mipmap/ic_launcher_round"
			android:supportsRtl="true"
			android:theme="@style/AppTheme">
		<meta-data android:name="com.syh.deeepoch.deeec.EcApp"
				android:value="IComponentConfig"/>
		<activity
				android:name=".DeeEcActivity"
				android:label="@string/title_activity_dee_ec"
				android:theme="@style/AppTheme.NoActionBar">
		</activity>
		<activity android:name=".EcGhostActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>

				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
	</application>

</manifest>